<Addin
	id="ClassicInterface"
	namespace="Do"
	version="1.0"
	name="Classic"
	description="A Quicksilver inspired interface for GNOME Do"
	author="Jason Smith"
	category="Hidden"
>
 
	<Runtime>
		<Import assembly="Do.Interface.Linux.Classic.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Interface.Linux" version="1.0" />
	</Dependencies>
	
	<!-- Extensions included in this assembly -->
	<!-- Sources -->
	<Extension path = "/Do/InterfaceWindow">
		<InterfaceWindow type="Do.Interface.AnimatedClassicWindow" />
	</Extension>
</Addin>
