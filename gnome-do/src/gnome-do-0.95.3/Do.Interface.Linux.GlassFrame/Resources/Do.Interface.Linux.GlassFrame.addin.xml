<Addin
	id="GlassInterface"
	namespace="Do"
	version="1.0"
	name="Glass"
	description="A KDE inspired interface for Do"
	category="Hidden"
	author="Jason Smith"
>
 
	<Runtime>
		<Import assembly="Do.Interface.Linux.GlassFrame.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Interface.Linux" version="1.0" />
	</Dependencies>
	
	<!-- Extensions included in this assembly -->
	<!-- Sources -->
	<Extension path = "/Do/InterfaceWindow">
		<InterfaceWindow type="Do.Interface.GlassFrameWindow" />
	</Extension>
</Addin>
